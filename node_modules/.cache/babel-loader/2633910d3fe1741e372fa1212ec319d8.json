.chart = null;\n  },\n  methods: {\n    initChart() {\n      this.chart = echarts.init(this.$el, \"macarons\");\n      this.chart.showLoading();\n      this.axios.get(\"http://\"+axiosURL+\"/api/findByMonth\").then((res) => {\n        let data1 = res.data;\n        let dataAxis = [], data = [];\n        let maxNum = 0;\n        for (let key in data1) {\n          let proName = key;\n          let num = data1[key];\n          dataAxis.push(proName);\n          data.push(num);\n        }\n        this.chart.hideLoading();\n\n        this.chart.setOption({\n                     toolbox: {\n            show: true,\n            feature: {\n              dataView: { show: true, readOnly: false },\n              magicType: { show: true, type: [\"line\", \"bar\"] },\n              restore: { show: true },\n              saveAsImage: { show: true },\n            },\n          },\n          xAxis: {\n            type: \"category\",\n            data: [\"Jan\",\"Feb\",\"Mar\",\"Apr\",\"May\",\"Jun\",\"Jul\",\"Aug\",\"Sep\",\"Oct\",\"Nov\", \"Dec\"],\n          },\n          yAxis: {\n            type: \"value\",\n          },\n          series: [\n            {\n              data: data,\n              type: \"line\",\n            },\n          ],\n        });\n      });\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views/charts/components"}]}